{
  "printWidth": 80,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "semi": true,
  "tabWidth": 2,
  "endOfLine": "lf",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-organize-imports"
  ],
  "importOrder": ["<THIRD_PARTY_MODULES>", "^@/*", "^[./]"],
  "importOrderSeparation": true
}

